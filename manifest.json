{
    "manifest_version":2,
    "version":"1.0",
    "name": "Awesome Test Extension",
    

    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },

    "content_scripts": [
        {
          "matches": ["https://*.github.com/*"],
          "css": ["newStyle.css"],
          "js": ["content.js"]
        }
      ],

    "permissions": [
        "storage",
        "activeTab",
        "webNavigation"
    ]

  }